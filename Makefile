	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/doc/copyright 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/doc/wallch
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/doc/ 


install_imagefiles: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/pixmaps/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/pixmaps/ 
	cp data/mime/128/application-x-wallch-project.png /usr/share/icons/hicolor/128x128/mimetypes/; cp data/mime/64/application-x-wallch-project.png /usr/share/icons/hicolor/64x64/mimetypes/; cp data/mime/48/application-x-wallch-project.png /usr/share/icons/hicolor/48x48/mimetypes/; cp data/mime/32/application-x-wallch-project.png /usr/share/icons/hicolor/32x32/mimetypes/; cp data/mime/22/application-x-wallch-project.png /usr/share/icons/hicolor/22x22/mimetypes/; cp data/mime/16/application-x-wallch-project.png /usr/share/icons/hicolor/16x16/mimetypes/; cp data/mime/wallch.xml /usr/share/mime/packages/; cp data/translations/wallch_gr.qm /etc/wallch/translations/
	-$(INSTALL_FILE) /home/maxmahem/wallch/Wallch-Fork/data/pixmap/wallch.png $(INSTALL_ROOT)/usr/share/pixmaps/


uninstall_imagefiles:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/pixmaps/wallch.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/pixmaps/ 


install:  install_configfiles install_shortcutfiles install_helpfiles install_binfiles install_manfiles install_docfiles install_imagefiles  FORCE

uninstall: uninstall_configfiles uninstall_shortcutfiles uninstall_helpfiles uninstall_binfiles uninstall_manfiles uninstall_docfiles uninstall_imagefiles   FORCE

FORCE:

